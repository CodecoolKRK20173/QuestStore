{% extends "../home.twig" %}

{% block nickname %}{% endblock %}

{% block mentorsBtnClass %}class="active"{% endblock %}

{% block content %}
    <link rel="stylesheet" href="../static/css/addMentor.css">
    <form method="post">
        <p>Nickname:</p>
        <input name="nickname" type="text">
        <p>Password:</p>
        <input name="password" type="text">
        <p>First Name:</p>
        <input name="firstName" type="text">
        <p>Last Name:</p>
        <input name="lastName" type="text">
        <p>E-mail:</p>
        <input name="email" type="text"><br>
        <input type="submit">
    </form>
{% endblock %}